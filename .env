VITE_APP_NAME=player-with-upload
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=MP3 Player with Upload and Visualizer

# Audio Configuration
VITE_MAX_FILE_SIZE=50000000
VITE_SUPPORTED_FORMATS=audio/mp3,audio/wav,audio/ogg,audio/m4a,audio/aac
VITE_DEFAULT_VOLUME=0.7
VITE_FADE_DURATION=300

# Visualization Settings
VITE_VISUALIZER_FFT_SIZE=2048
VITE_VISUALIZER_SMOOTHING=0.8
VITE_VISUALIZER_BAR_COUNT=64
VITE_VISUALIZER_MIN_DECIBELS=-90
VITE_VISUALIZER_MAX_DECIBELS=-10

# UI Configuration
VITE_THEME_PRIMARY=#3b82f6
VITE_THEME_SECONDARY=#8b5cf6
VITE_THEME_ACCENT=#06d6a0
VITE_GLASSMORPHISM_BLUR=20px
VITE_GLASSMORPHISM_OPACITY=0.1

# Performance Settings
VITE_CANVAS_DPI_SCALE=2
VITE_ANIMATION_FPS=60
VITE_DEBOUNCE_DELAY=300

# Development Settings
VITE_DEBUG_MODE=false
VITE_ENABLE_CONSOLE_LOGS=false

# External Resources
VITE_PLACEHOLDER_ALBUM_ART=https://via.placeholder.com/300x300/3b82f6/ffffff?text=Music
VITE_DEFAULT_BACKGROUND=https://images.unsplash.com/photo-1470225620780-dba8ba36b745?w=1920&h=1080&fit=crop